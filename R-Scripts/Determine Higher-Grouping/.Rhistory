plot(PCA$pc.scores[,1],PCA$pc.scores[,2], col=my_colors, asp=T, las=1,pch=16, cex=1.5)
legend("topright", legend= levels(subfam1), col= c("goldenrod1", "black", "blueviolet", "violet", "chartreuse3"), pch=rep(19,14) )
